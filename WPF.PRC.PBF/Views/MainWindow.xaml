<catel:Window
    x:Class="WPF.PRC.PBF.Views.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:catel="http://schemas.catelproject.com"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
    xmlns:pbf="clr-namespace:WPF.PRC.PBF"
    xmlns:userControls="clr-namespace:WPF.PRC.PBF.Views.UserControls"
    ResizeMode="CanResize">



    <Grid x:Name="LayoutRoot">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <userControls:SuggestUserControl Grid.Row="0" EntityType="Citizenship" />
        <userControls:SuggestUserControl Grid.Row="1" EntityType="PlaceOfBirth" />

    </Grid>

</catel:Window>

